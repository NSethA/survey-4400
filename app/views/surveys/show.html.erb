<div class="container">
<p id="notice"><%= notice %></p>
<h1>Survey Data</h1>

<table class="table table-striped">
	<thead>
		<tr>
		<th>Name</th>
		<th>Laptop</th>
		<th>OS</th>
		<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<tr>
			<td><%= @survey.name %></td>
			<td><%= @survey.laptop %></td>
			<td><%= @survey.OS %></td>
		</tr>
	</tbody>
</table>
<%= link_to 'Edit', edit_survey_path(@survey) %> |
<%= link_to 'Back', surveys_path %>
</div>